<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm6ScheduleTypeCode="urn:un:unece:uncefact:codelist:standard:UNECE:ScheduleTypeCode:D10B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:ScheduleTypeCode:D10B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="ScheduleTypeCode" type="clm6ScheduleTypeCode:ScheduleTypeCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: ScheduleTypeCodeContentType           ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="ScheduleTypeCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="LOB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Line Of Balance</ccts:Name>
            <ccts:Description>Production delivery schedule events.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MFG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Manufacturing</ccts:Name>
            <ccts:Description>Manufacturing related tasks.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MSL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Milestone List</ccts:Name>
            <ccts:Description>A list of project milestones.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Network Schedule</ccts:Name>
            <ccts:Description>Project task and relationship details for network scheduling purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RNS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Resourced Network Schedule</ccts:Name>
            <ccts:Description>Project task with resource assignments and relationship details for network scheduling and resource leveling purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Task List</ccts:Name>
            <ccts:Description>A list of project tasks.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ZZZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mutually Defined</ccts:Name>
            <ccts:Description>Mutually agreed upon schedule type code.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
