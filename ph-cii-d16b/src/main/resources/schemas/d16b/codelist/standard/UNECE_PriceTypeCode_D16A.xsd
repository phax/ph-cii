<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm65375="urn:un:unece:uncefact:codelist:standard:UNECE:PriceTypeCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:PriceTypeCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="PriceTypeCode" type="clm65375:PriceTypeCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: PriceTypeCodeContentType              ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="PriceTypeCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="AA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cancellation price</ccts:Name>
            <ccts:Description>Price authorized to be charged in the event of an order being cancelled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Per ton</ccts:Name>
            <ccts:Description>To indicate that the price applies per ton.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Minimum order price</ccts:Name>
            <ccts:Description>A code to identify the price when the minimum number is purchased.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Export price</ccts:Name>
            <ccts:Description>A code to identify a price for the export market.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Range dependent price</ccts:Name>
            <ccts:Description>A code identifying the price for a specific range of purchase quantities.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Competitor price</ccts:Name>
            <ccts:Description>Price given by competitor.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Active ingredient</ccts:Name>
            <ccts:Description>The price is referring to the active ingredient.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>As is quantity</ccts:Name>
            <ccts:Description>The price is referring to the measured quantity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Catalogue</ccts:Name>
            <ccts:Description>Code specifying the catalogue price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Contract</ccts:Name>
            <ccts:Description>Code specifying the contract price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consumer unit</ccts:Name>
            <ccts:Description>The price is referring to the consumer unit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Distributor</ccts:Name>
            <ccts:Description>Code specifying the distributor price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>ECSC price</ccts:Name>
            <ccts:Description>Price registered at European Commission Steel and Carbon office (DG III).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net weight</ccts:Name>
            <ccts:Description>Code specifying the net weight price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Price catalogue</ccts:Name>
            <ccts:Description>Code specifying the catalogue price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Per each</ccts:Name>
            <ccts:Description>Code specifying the price per item.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Per kilogram</ccts:Name>
            <ccts:Description>Code specifying the price per kilogram.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Per litre</ccts:Name>
            <ccts:Description>Code specifying the price per litre.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Per tonne</ccts:Name>
            <ccts:Description>Code specifying the price per tonne.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Specified unit</ccts:Name>
            <ccts:Description>Code specifying the price per specified unit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Provisional price</ccts:Name>
            <ccts:Description>Code specifying a provisional price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gross weight</ccts:Name>
            <ccts:Description>Code specifying the gross weight price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="QT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Quoted</ccts:Name>
            <ccts:Description>Code specifying the quoted price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Suggested retail</ccts:Name>
            <ccts:Description>Code specifying the suggested retail price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>To be negotiated</ccts:Name>
            <ccts:Description>Code specifying that the price has to be negotiated.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Traded unit</ccts:Name>
            <ccts:Description>The price is referring to the traded unit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Theoretical weight</ccts:Name>
            <ccts:Description>Weight calculated on ordered dimension (length, width, thickness) not on final dimension (e.g. steel products).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="WH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Wholesale</ccts:Name>
            <ccts:Description>Code specifying the wholesale price.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="WI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gross volume</ccts:Name>
            <ccts:Description>The price is calculated based on gross volume.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
