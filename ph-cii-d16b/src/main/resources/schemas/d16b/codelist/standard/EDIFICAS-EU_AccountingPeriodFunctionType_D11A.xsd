<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm210AccountingE703="urn:un:unece:uncefact:codelist:standard:EDIFICAS-EU:AccountingPeriodFunctionType:D11A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:EDIFICAS-EU:AccountingPeriodFunctionType:D11A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="AccountingPeriodFunctionType" type="clm210AccountingE703:AccountingPeriodFunctionTypeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: AccountingPeriodFunctionTypeContentType ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="AccountingPeriodFunctionTypeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="246">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Accounting year</ccts:Name>
            <ccts:Description>Year considered for accounting of the treaty or portion of the treaty.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="263">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Invoicing period</ccts:Name>
            <ccts:Description>Period for which an invoice is issued.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="322">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Accounting period</ccts:Name>
            <ccts:Description>A period of time for the recording of financial transactions for accounting.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="325">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tax period</ccts:Name>
            <ccts:Description>Period a tax rate/tax amount etc. is applicable.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="452">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Trial balance period</ccts:Name>
            <ccts:Description>Period covered by the trial balance.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="456">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Chart of account period</ccts:Name>
            <ccts:Description>Period covered by the chart of account.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="492">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Balance data/time/period</ccts:Name>
            <ccts:Description>The date/time/period of a balance.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="567">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Report period</ccts:Name>
            <ccts:Description>Period covered by the report.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="E001">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fiscal consideration period</ccts:Name>
            <ccts:Description>A period of time for fiscal consideration.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
