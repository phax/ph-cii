<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm67233AutomaticDataCaptureMethodCode="urn:un:unece:uncefact:codelist:standard:UNECE:AutomaticDataCaptureMethodCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:AutomaticDataCaptureMethodCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.2">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="AutomaticDataCaptureMethodCode" type="clm67233AutomaticDataCaptureMethodCode:AutomaticDataCaptureMethodCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: AutomaticDataCaptureMethodCodeContentType ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="AutomaticDataCaptureMethodCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="50">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package barcoded EAN-13 or EAN-8</ccts:Name>
            <ccts:Description>The package is barcoded with EAN-13 or EAN-8 code.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="51">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package barcoded ITF-14 or ITF-6</ccts:Name>
            <ccts:Description>The package is barcoded with ITF-14 or ITF-6 code.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="52">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package barcoded UCC or EAN-128</ccts:Name>
            <ccts:Description>The package is barcoded with UCC or EAN-128 code.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="64">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package never EPC tagged</ccts:Name>
            <ccts:Description>The package will never be tagged with an EPC (Electronic Product Code) transponder.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="65">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package sometimes EPC tagged</ccts:Name>
            <ccts:Description>The package will sometimes be tagged with an EPC (Electronic Product Code) transponder.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="67">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tagging/bar code instructions</ccts:Name>
            <ccts:Description>The information provides instructions as to how a package is to be tagged or have a bar code applied to it.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="78">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package bar-coded and EPC tagged</ccts:Name>
            <ccts:Description>The package is bar-coded and tagged with an EPC (Electronic Product Code) transponder.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="79">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package EPC tagged only</ccts:Name>
            <ccts:Description>The package is tagged with an EPC (Electronic Product Code) transponder only.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="81">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package marked with a variable measure barcode</ccts:Name>
            <ccts:Description>The package is marked with a variable measure barcode.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="82">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Package marked with fixed measure barcode.</ccts:Name>
            <ccts:Description>The package is marked with a fixed measure barcode.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
