<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm66245="urn:un:unece:uncefact:codelist:standard:UNECE:TemperatureTypeCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:TemperatureTypeCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.3">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="TemperatureTypeCode" type="clm66245:TemperatureTypeCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: TemperatureTypeCodeContentType        ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="TemperatureTypeCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Storage temperature</ccts:Name>
            <ccts:Description>[6240] The temperature at which the cargo is to be kept while it is in storage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport temperature</ccts:Name>
            <ccts:Description>[6242] The temperature at which cargo is to be kept while it is under transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cargo operating temperature</ccts:Name>
            <ccts:Description>The temperature at which cargo is to be kept during cargo handling.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport emergency temperature</ccts:Name>
            <ccts:Description>The temperature at which emergency procedures apply for the disposal of temperature-controlled goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport control temperature</ccts:Name>
            <ccts:Description>The maximum temperature at which certain products can be safely transported.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Boiling point</ccts:Name>
            <ccts:Description>The  temperature at which a liquid begins to boil.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Temperature, recorded</ccts:Name>
            <ccts:Description>The recorded temperature.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Self-accelerating decomposition temperature (SADT)</ccts:Name>
            <ccts:Description>The lowest temperature at which self-accelerating decomposition may occur for a substance in the packaging as used in transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
