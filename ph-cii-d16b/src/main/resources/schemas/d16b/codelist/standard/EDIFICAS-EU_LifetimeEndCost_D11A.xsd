<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm210LifetimeEndCostE206="urn:un:unece:uncefact:codelist:standard:EDIFICAS-EU:LifetimeEndCost:D11A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:EDIFICAS-EU:LifetimeEndCost:D11A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="LifetimeEndCost" type="clm210LifetimeEndCostE206:LifetimeEndCostContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: LifetimeEndCostContentType            ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="LifetimeEndCostContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of restoration of site</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of restoration of site.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of dismantling of site</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of dismantling of site.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of discontinuation of fixed asset</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of discontinuation of fixed asset.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of stake in the rubbish of fixed asset</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of stake in the rubbish of fixed asset.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of compensation in third parties</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of compensation in third parties.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Previsional cost of the damage caused to the environment</ccts:Name>
            <ccts:Description>The cost of end lifetime is a previsional cost of the damage caused to the environment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of elimination of the waste and the efforts undertaken to limit its quantity</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of elimination of the waste and the efforts undertaken to limit its quantity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of wrestling against the soil pollution, waters of surface and some subterranean waters</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of wrestling against the soil pollution, waters of surface and some subterranean waters.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of conservation of the air quality and the climate</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of conservation of the air quality and the climate.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of the reduction of the sound broadcasts</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of the reduction of the sound broadcasts.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cost of the protection of the biodiversity and the landscape</ccts:Name>
            <ccts:Description>The cost of end lifetime is a cost of the protection of the biodiversity and the landscape.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
