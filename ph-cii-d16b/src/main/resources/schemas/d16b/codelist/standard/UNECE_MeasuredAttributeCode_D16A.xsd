<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm66313="urn:un:unece:uncefact:codelist:standard:UNECE:MeasuredAttributeCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:MeasuredAttributeCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.7">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="MeasuredAttributeCode" type="clm66313:MeasuredAttributeCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: MeasuredAttributeCodeContentType      ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="MeasuredAttributeCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="A">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consolidated weight</ccts:Name>
            <ccts:Description>The measured consolidated weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net weight</ccts:Name>
            <ccts:Description>[6160] Weight of goods including any packaging that normally going with the goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Goods item gross weight</ccts:Name>
            <ccts:Description>[6292] Weight (mass) of goods including packing but excluding the carrier's equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Total net weight</ccts:Name>
            <ccts:Description>Total weight of goods excluding packaging.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consignment gross weight</ccts:Name>
            <ccts:Description>[6012] Total gross weight (mass) of all goods items referred to as one consignment including packaging but excluding any transport equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net net weight</ccts:Name>
            <ccts:Description>[6048] Weight (mass) if goods without any packaging.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stern thrust</ccts:Name>
            <ccts:Description>Force exerted by a thruster installed at the stern of a vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bow thrust</ccts:Name>
            <ccts:Description>Force exerted by a thruster installed at the bow of a vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Hydrate content of an alcoholic product at bottling</ccts:Name>
            <ccts:Description>The hydrate content of an alcoholic product at the moment of bottling.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Number of units per pallet</ccts:Name>
            <ccts:Description>The number of units contained on a pallet.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fat content</ccts:Name>
            <ccts:Description>An indication of the fat content of a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport means gross weight</ccts:Name>
            <ccts:Description>[6300] The measure of the overall size of a ship determined in accordance with the provisions of the International Convention on Tonnage Measurement of Ships, 1969.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net tonnage of the vessel</ccts:Name>
            <ccts:Description>[6302] The measure of the useful capacity of a ship determined in accordance with the provisions of the International Convention on Tonnage Measurement of Ships, 1969.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Humidity</ccts:Name>
            <ccts:Description>Degree of moisture.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Voltage</ccts:Name>
            <ccts:Description>Electromotive force, or difference of electronic potential between two points.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Power consumption</ccts:Name>
            <ccts:Description>Value of energy consumption.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Heat dissipation</ccts:Name>
            <ccts:Description>Measurement of the rate of dispersal of heat.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Air flow</ccts:Name>
            <ccts:Description>Measurement of the flow of air.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Shock impact</ccts:Name>
            <ccts:Description>Measurement of the impact of a shock.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Operative temperature</ccts:Name>
            <ccts:Description>Temperature identified system or process works according to specifications.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Non operative temperature</ccts:Name>
            <ccts:Description>Temperature identified system or process does not work according to specifications.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gross volume</ccts:Name>
            <ccts:Description>The volume unadjusted for factors such as temperature or gravity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net volume</ccts:Name>
            <ccts:Description>The volume after adjustment for factors such as temperature or gravity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Water content</ccts:Name>
            <ccts:Description>Water content in product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AAZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tensile stress</ccts:Name>
            <ccts:Description>The measured tensile stress.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fibrosity</ccts:Name>
            <ccts:Description>The measured fibrosity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gauge length</ccts:Name>
            <ccts:Description>The measured gauge length.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Radius</ccts:Name>
            <ccts:Description>The measured radius.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Straightness</ccts:Name>
            <ccts:Description>Straightness of the item.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Strain</ccts:Name>
            <ccts:Description>The measured strain.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Item width when unrolled</ccts:Name>
            <ccts:Description>The width of an item when unrolled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Item length when unrolled</ccts:Name>
            <ccts:Description>The length of an item when unrolled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Item area when unrolled</ccts:Name>
            <ccts:Description>The area occupied by an item when unrolled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Original wort</ccts:Name>
            <ccts:Description>Measure of the malt and hops content of beer, before fermentation has taken place.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Volume</ccts:Name>
            <ccts:Description>The amount of air space taken up by the entity identified in the 6311 qualifier.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Angle</ccts:Name>
            <ccts:Description>The angle of an object.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Peg hole horizontal distance from package leftmost edge</ccts:Name>
            <ccts:Description>Horizontal distance from the left most edge of the package to the center of the hole into which the peg is inserted.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Peg hole vertical distance from package top</ccts:Name>
            <ccts:Description>Vertical distance from the top of the package to the top of the hole into which the peg is inserted.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Number of layers per pallet</ccts:Name>
            <ccts:Description>The number of layers per pallet.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Product strengh, chemical</ccts:Name>
            <ccts:Description>The amount of the single active chemical ingredient within a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Product strength basis, chemical</ccts:Name>
            <ccts:Description>Amount of product used as the basis for the specification of the chemical product strenght.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Item weight</ccts:Name>
            <ccts:Description>Weight at line item level.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Payload weight, maximum</ccts:Name>
            <ccts:Description>The maximum weight of a payload.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Weight of conveyance</ccts:Name>
            <ccts:Description>Tonnage of conveyance.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Conveyance summer dead weight</ccts:Name>
            <ccts:Description>Registered summer dead weight total tonnage of the vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ABZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Containerized cargo on vessel's weight</ccts:Name>
            <ccts:Description>Total weight of containerized cargo on vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Non-containerized cargo on vessel's weight</ccts:Name>
            <ccts:Description>Total weight of non-containerized cargo on vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ascertained weight</ccts:Name>
            <ccts:Description>[4240] Endorsement of the true weight (mass) as ascertained or verified.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Chargeable weight</ccts:Name>
            <ccts:Description>The weight on which charges are based.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Estimated gross weight</ccts:Name>
            <ccts:Description>Estimated weight (mass) of goods, including packing and excluding carrier's.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Estimated volume</ccts:Name>
            <ccts:Description>Estimated size or measure of anything in three dimensions.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Vessel overall length</ccts:Name>
            <ccts:Description>Total overall length of the vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loading meters</ccts:Name>
            <ccts:Description>[6046] (6042) (6044) The length in a vehicle, whereby the complete width and height over that length is needed for the goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Number of axles</ccts:Name>
            <ccts:Description>Number of axles of movable equipment or means of transport on wheels.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ACX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Payload</ccts:Name>
            <ccts:Description>The revenue-producing load carried by a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Start position in the length</ccts:Name>
            <ccts:Description>The starting position from the beginning of an item located in the length direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>End position in the length</ccts:Name>
            <ccts:Description>The end position from the beginning of an item located in the length direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Start position in the width</ccts:Name>
            <ccts:Description>The start position from the beginning of an item located in the width direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>End position in the width</ccts:Name>
            <ccts:Description>The end position from the beginning of an item located in the width direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Start position in the thickness</ccts:Name>
            <ccts:Description>The start position from the beginning of an item located in the thickness direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>End position in the thickness</ccts:Name>
            <ccts:Description>The end position from the beginning of an item located in the thickness direction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport container actual filling weight</ccts:Name>
            <ccts:Description>Actual filling weight of a transport container.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport container maximum capacity</ccts:Name>
            <ccts:Description>Maximum capacity of a transport container.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ADZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Declared net weight</ccts:Name>
            <ccts:Description>The declared net weight of a product or products used for invoicing, customs or transport purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loading height</ccts:Name>
            <ccts:Description>Maximum height of products or packages loaded onto a given transportation device or equipment such as a pallet.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stacking height</ccts:Name>
            <ccts:Description>Maximum height up to which the same product or package may be placed one upon the other for storage purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Calculated weight</ccts:Name>
            <ccts:Description>The calculated weight of the item based on the ordered dimensions.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AED">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ferrite</ccts:Name>
            <ccts:Description>The chemical composition ferrite.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Impurity</ccts:Name>
            <ccts:Description>The impurity of the product i.e. the measurement of other chemical elements not normally appearing in a product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Grain size</ccts:Name>
            <ccts:Description>The grain size.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Lanthanides</ccts:Name>
            <ccts:Description>The chemical element Lanthanides.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Elasticity</ccts:Name>
            <ccts:Description>The value of the elasticity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Drained weight</ccts:Name>
            <ccts:Description>The weight of a product when all liquids used in the packaging of the product have been removed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gallium</ccts:Name>
            <ccts:Description>Measurement of the gallium component.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Strontium</ccts:Name>
            <ccts:Description>Measurement of the strontium component.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Area</ccts:Name>
            <ccts:Description>Extent or measure of a surface.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment storage limitation</ccts:Name>
            <ccts:Description>Maximum storage limit of the equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Radioactive index of transport</ccts:Name>
            <ccts:Description>The index of transport determines the maximum radiation level at a distance of 1m from the external surface.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Radioactivity</ccts:Name>
            <ccts:Description>Activity of radioactive material.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Average gross weight</ccts:Name>
            <ccts:Description>Weight which is the outcome of the division of the total gross weight by the number of units.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Forward draft</ccts:Name>
            <ccts:Description>Depth of water from the surface of water to the bottom of the vessel measured at the draft mark of the bow.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AER">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>After draft</ccts:Name>
            <ccts:Description>Depth of water from the surface of water to the bottom of the vessel measured at the draft mark of the stern.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AES">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acidity</ccts:Name>
            <ccts:Description>The quality, state, or degree of being acid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AET">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Transport equipment gross weight</ccts:Name>
            <ccts:Description>[6294] Weight of a transport equipment including the cargo and carrier's equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Total transport equipment gross weight</ccts:Name>
            <ccts:Description>Total weight of all transport equipment including the cargo and carrier's equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acidity of juice</ccts:Name>
            <ccts:Description>Acid measurement of juice.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Penetrometry</ccts:Name>
            <ccts:Description>Measurement of force required to drive a standard penetrating stamp.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Durofel</ccts:Name>
            <ccts:Description>Measurement of the elastic force using a standard penetrating stamp.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Juice weight per 100 grams</ccts:Name>
            <ccts:Description>Measurement of weight of juice, based on 100 grams of the entire weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AEZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fruit skin colour</ccts:Name>
            <ccts:Description>Measurement of the colouring of the epidermis of a fruit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Angle of bend</ccts:Name>
            <ccts:Description>The measured angle of bend.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Fixed incremental measurement</ccts:Name>
            <ccts:Description>The measurement of the fixed increment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Durofel D10</ccts:Name>
            <ccts:Description>Measure of the elastic force of the pulp of a fruit. It is measured with a penetrating stamp with diameter 10.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Durofel D25</ccts:Name>
            <ccts:Description>Measure of the elastic force of the pulp of a fruit. It is measured with a penetrating stamp with diameter 25.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Durofel D50</ccts:Name>
            <ccts:Description>Measure of the elastic force of the pulp of a fruit. It is measured with a penetrating stamp with diameter 50.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Maximum stacking weight</ccts:Name>
            <ccts:Description>The maximum weight which may be stacked upon a product or package without the product or packaging being crushed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Gross measure cube</ccts:Name>
            <ccts:Description>The total cubic space occupied by an item, taking into account any protruding components, arrived at by multiplying the maximum length, width and height.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Percentage fat content in dry matter</ccts:Name>
            <ccts:Description>The percentage of fat content in dry matter.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Saccharometric content</ccts:Name>
            <ccts:Description>Measurement of the sugar content of a solution.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Hydrate content of an alcoholic product after bottling</ccts:Name>
            <ccts:Description>The hydrate content which occurs in an alcoholic product after bottling.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Anhydrous content</ccts:Name>
            <ccts:Description>The non-water content.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Certified weight</ccts:Name>
            <ccts:Description>Weight which has been certified.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Freeboard</ccts:Name>
            <ccts:Description>The vertical distance between the waterline and the upper edge of the deck line.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Maximum vessel draught</ccts:Name>
            <ccts:Description>The depth of water needed to float the ship (Maximum vessel draught according to Load Line Certificate IMO, MSC/Circ. 920 15 June 1999).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Net explosive weight</ccts:Name>
            <ccts:Description>Mass of the explosive part or substance of goods without any packing.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Radioactive criticality safety index</ccts:Name>
            <ccts:Description>A number, assigned to fissile material i.e. material capable of sustaining a nuclear chain reaction, which is used to provide control over the accumulation of packages containing such material.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Waste currently on board</ccts:Name>
            <ccts:Description>Amount of waste on board at the moment of the notification.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Waste to be delivered at waste reception facility</ccts:Name>
            <ccts:Description>Amount of waste to be delivered to a waste reception facility, e.g. in the port of call.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Waste to be generated until next port of call, estimated</ccts:Name>
            <ccts:Description>Estimated amount of waste to be generated between creation of the current notification and arrival in the next port of call, irrespective of use of incinerator or compactor or disposal at sea.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Waste remaining on board at departure</ccts:Name>
            <ccts:Description>Amount of waste remaining on board when leaving the port of call.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Colour depth</ccts:Name>
            <ccts:Description>The number of distinct colours represented, e.g. in an image or in a display.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Colour depth, maximum</ccts:Name>
            <ccts:Description>The maximum number of distinct colours that can be represented, e.g. in an image or in a display.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Image resolution</ccts:Name>
            <ccts:Description>The level of detail of an image.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Device resolution, maximum</ccts:Name>
            <ccts:Description>The maximum level of detail produced by a device.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AFX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acoustic absorption coefficient</ccts:Name>
            <ccts:Description>The portion of sound energy a surface absorbs, measured at different frequencies.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Billed weight</ccts:Name>
            <ccts:Description>The measured billed weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Breaking load</ccts:Name>
            <ccts:Description>The measured breaking load.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BMY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Platinum</ccts:Name>
            <ccts:Description>The measurement of the platinum component.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BMZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Silver</ccts:Name>
            <ccts:Description>The measurement of the silver component.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>List</ccts:Name>
            <ccts:Description>The leaning or inclination of a vessel expressed in degrees port or starboard.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Trim</ccts:Name>
            <ccts:Description>The condition of a vessel with reference to its longitudinal axis.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Free water</ccts:Name>
            <ccts:Description>The volume of water present in a container that is not in suspension in the contained liquid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BND">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bands</ccts:Name>
            <ccts:Description>The measured bands.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>API (American Petroleum Institute) gravity</ccts:Name>
            <ccts:Description>The relative density of petroleum liquids as specified by a standard developed by the API.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum gross observed volume</ccts:Name>
            <ccts:Description>The total volume of all petroleum liquids and sediment and water, excluding free water, at observed temperature and pressure.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum gross standard volume</ccts:Name>
            <ccts:Description>The total volume of all petroleum liquids, sediment, and water excluding free water, corrected by the appropriate volume correction factor for the observed temperature and American Petroleum Institute relative density, or density to a standard temperature.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Volume variance</ccts:Name>
            <ccts:Description>The change in the volume measurement.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum net standard volume</ccts:Name>
            <ccts:Description>The total volume of all petroleum liquids, excluding sediment and water and free water, corrected by the appropriate volume correction factor for the observed temperature and American Petroleum Institute gravity relative to density or to a standard temperature.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Material on-board quantity, after discharge</ccts:Name>
            <ccts:Description>The material in vessel tanks, void spaces, and pipelines after discharge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum total calculated volume</ccts:Name>
            <ccts:Description>The total volume of all petroleum liquids, sediment and water corrected by the appropriate volume correction factor for the observed temperature and American Petroleum Institute (API) gravity, relative density, or density to a standard temperature.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum total observed volume</ccts:Name>
            <ccts:Description>The total volume of all petroleum liquids, sediment and water and free water at observed temperature and pressure.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Innage gauge distance</ccts:Name>
            <ccts:Description>The measured distance from the surface of the liquid to a fixed datum plate or to the tank bottom.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Petroleum net standard weight</ccts:Name>
            <ccts:Description>The total weight of all petroleum liquids excluding sediments, water and free water.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sediment and water in petroleum</ccts:Name>
            <ccts:Description>The measurement of non-hydrocarbon solid material and water in suspension in petroleum liquid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Observed reference height, tank</ccts:Name>
            <ccts:Description>The observed distance from the tank bottom or datum plate to the established reference point.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reference height, tank</ccts:Name>
            <ccts:Description>The measured distance from the tank bottom or datum plate to the established reference point.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ullage gauge distance</ccts:Name>
            <ccts:Description>The measured distance from the cargo liquid surface to the reference point.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Trim correction</ccts:Name>
            <ccts:Description>The correction applied to the observed gauge or observed volume when a vessel is not on an even keel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bow to bridge distance</ccts:Name>
            <ccts:Description>The distance between the bow and the bridge of a vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Peg hole number</ccts:Name>
            <ccts:Description>Used to identify the peg hole if more than one hole is present in the product or packaging.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Number of inner packs</ccts:Name>
            <ccts:Description>Indicates the number of non coded physical groupings (inner packs) of next lower level trade items within the current trade item level.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BNW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Number of next level trade items within inner pack</ccts:Name>
            <ccts:Description>The number of next lower level trade items contained within the physical non-coded grouping (inner pack).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
