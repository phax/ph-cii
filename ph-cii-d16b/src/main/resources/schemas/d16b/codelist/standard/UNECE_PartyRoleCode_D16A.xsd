          <xsd:documentation xml:lang="en">
            <ccts:Name>Secretariat</ccts:Name>
            <ccts:Description>Party is a secretariat.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LU">
