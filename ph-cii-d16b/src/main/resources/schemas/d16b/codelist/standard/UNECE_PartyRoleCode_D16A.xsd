          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DFS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Certificate issuer, veterinary</ccts:Name>
            <ccts:Description>The person who issues veterinary certificates.</ccts:Description>
