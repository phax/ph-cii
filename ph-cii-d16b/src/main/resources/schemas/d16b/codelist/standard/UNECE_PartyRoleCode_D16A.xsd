          <xsd:documentation xml:lang="en">
            <ccts:Name>Contact party 1</ccts:Name>
            <ccts:Description>First party to contact.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="P2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Contact party 2</ccts:Name>
            <ccts:Description>Second party to contact.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="P3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Contact party 3</ccts:Name>
            <ccts:Description>Third party to contact.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="P4">
