        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DDM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Grid operator</ccts:Name>
            <ccts:Description>A party operating a grid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DDN">
