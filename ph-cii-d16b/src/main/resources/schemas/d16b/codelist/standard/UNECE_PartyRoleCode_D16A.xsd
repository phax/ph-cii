          <xsd:documentation xml:lang="en">
            <ccts:Name>Lessee's surveyor</ccts:Name>
            <ccts:Description>Surveyor hired by the lessee of the item.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
