          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pilotage service provider</ccts:Name>
            <ccts:Description>Party responsible for the pilotage of the vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Berth towage service provider</ccts:Name>
