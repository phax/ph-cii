          <xsd:documentation xml:lang="en">
            <ccts:Name>Third party</ccts:Name>
            <ccts:Description>Another party besides the two principals.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Receiver's sub-entity</ccts:Name>
