        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DFT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Assembler</ccts:Name>
            <ccts:Description>The party putting together various components or parts into a product or commodity.</ccts:Description>
