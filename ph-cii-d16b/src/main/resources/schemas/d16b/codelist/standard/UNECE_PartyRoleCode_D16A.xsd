          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Party connected to grid</ccts:Name>
            <ccts:Description>A party that contracts for the right to consume or produce electricity at a metering point.</ccts:Description>
