          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Party data responsible party</ccts:Name>
            <ccts:Description>The party responsible for all party data.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OD">
