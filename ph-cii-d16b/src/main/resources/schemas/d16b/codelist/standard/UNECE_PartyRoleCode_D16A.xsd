      <xsd:enumeration value="DEG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consortium Carrier (maritime)</ccts:Name>
            <ccts:Description>A marine carrier that transports goods for more than one shipping line between named points.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
