        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DFQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Security certificate issuer, recognized</ccts:Name>
