          <xsd:documentation xml:lang="en">
            <ccts:Name>Profile maintenance party</ccts:Name>
            <ccts:Description>A party that maintains profiles.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stowaway</ccts:Name>
            <ccts:Description>A person who hides on a conveyance in order to obtain free passage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEF">
