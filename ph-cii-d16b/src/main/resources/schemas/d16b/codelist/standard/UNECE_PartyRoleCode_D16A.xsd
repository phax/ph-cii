          <xsd:documentation xml:lang="en">
            <ccts:Name>Owner of property</ccts:Name>
            <ccts:Description>Party owning a property.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OF">
