        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ON">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Remitting bank</ccts:Name>
            <ccts:Description>The bank to which the principal has entrusted the handling of a collection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
