          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment repair party</ccts:Name>
            <ccts:Description>A party making repairs to equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="OE">
