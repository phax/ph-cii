          <xsd:documentation xml:lang="en">
            <ccts:Name>Depositor</ccts:Name>
            <ccts:Description>Party depositing goods, financial payments or documents.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Metered data aggregator</ccts:Name>
            <ccts:Description>A party responsible for aggregation of metered data.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DEB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Meter operator</ccts:Name>
