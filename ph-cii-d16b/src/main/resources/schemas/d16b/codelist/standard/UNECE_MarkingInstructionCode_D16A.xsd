<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm64233="urn:un:unece:uncefact:codelist:standard:UNECE:MarkingInstructionCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:MarkingInstructionCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.4">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="MarkingInstructionCode" type="clm64233:MarkingInstructionCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: MarkingInstructionCodeContentType     ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="MarkingInstructionCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Do not mark suppliers company name</ccts:Name>
            <ccts:Description>Packaging should not be marked with supplier's company name.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark customers company name</ccts:Name>
            <ccts:Description>Packaging should be marked with customer's company name.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark customers references</ccts:Name>
            <ccts:Description>Packaging should be marked with customer's references.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark additionally customers article description</ccts:Name>
            <ccts:Description>Packaging should also be marked with customer's article description.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark exclusively customers article description</ccts:Name>
            <ccts:Description>Packaging should be marked with customer's article description only.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark packages dimensions</ccts:Name>
            <ccts:Description>Packaging should be marked with package's dimensions.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark net weight</ccts:Name>
            <ccts:Description>Packaging should be marked with net weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark gross weight</ccts:Name>
            <ccts:Description>Packaging should be marked with gross weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark tare weight</ccts:Name>
            <ccts:Description>Packaging should be marked with tare weight.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark batch number</ccts:Name>
            <ccts:Description>Packaging should be marked with batch number.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark article number customer</ccts:Name>
            <ccts:Description>Packaging should be marked with customer's article number.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark running number of packages</ccts:Name>
            <ccts:Description>Packaging should be marked with the running number of packages.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark date of production</ccts:Name>
            <ccts:Description>Packaging should be marked with the date of production.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark expiry date</ccts:Name>
            <ccts:Description>Packaging should be marked with the expiry date.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark supplier number</ccts:Name>
            <ccts:Description>Packaging should be marked with the supplier number.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Buyer's instructions</ccts:Name>
            <ccts:Description>Markings as specified by the buyer.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Seller's instructions</ccts:Name>
            <ccts:Description>Markings as specified by the seller.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Carrier's instructions</ccts:Name>
            <ccts:Description>Markings as specified by carrier.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Legal requirements</ccts:Name>
            <ccts:Description>Markings as specified by law.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Industry instructions</ccts:Name>
            <ccts:Description>Markings as specified by industry.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Line item only</ccts:Name>
            <ccts:Description>Exclusive reference markings for this line.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pre-marked by buyer</ccts:Name>
            <ccts:Description>Packaging should be pre-marked by buyer.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Entire shipment</ccts:Name>
            <ccts:Description>Markings refer to the entire shipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Shipper assigned</ccts:Name>
            <ccts:Description>Markings to identify a shipment, package or carton as assigned by shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Shipper assigned roll number</ccts:Name>
            <ccts:Description>Markings to identify a roll as assigned by the shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Shipper assigned skid number</ccts:Name>
            <ccts:Description>Markings used to identify a skid as assigned by shipper.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Uniform Code Council (UCC) format</ccts:Name>
            <ccts:Description>Markings according to UCC format are required. System of coding products where by each item/multipack case type is uniquely identified. A unique manufacturer Id is assigned by the UCC.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark free text</ccts:Name>
            <ccts:Description>Packaging should be marked with free text.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark case number</ccts:Name>
            <ccts:Description>Case numbers to be used for marking.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark serial shipping container code</ccts:Name>
            <ccts:Description>Requests the marking of the serial shipping container code to the transport container or packaging.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with ISO 10374 (permanent container tag)</ccts:Name>
            <ccts:Description>The container has been marked with a permanent container tag according to ISO 10374.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark date of packaging</ccts:Name>
            <ccts:Description>Packaging must be marked with the packaging date.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mark reference to source entity</ccts:Name>
            <ccts:Description>Packaging must be marked with the reference to source entity. The reference to source identifies that the trade item was derived from.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked GS1 Global Individual Asset Identifier</ccts:Name>
            <ccts:Description>Indication that the GS1 Global Individual Asset Identifier has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="35">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with a product serial number</ccts:Name>
            <ccts:Description>Indication that the product serial number has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="36">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with a GS1 Global Identification Number for Consignment (GINC)</ccts:Name>
            <ccts:Description>Indication that the GS1 Global Identification Number for Consignment has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="37">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with a GS1 Global Shipment Identification Number (GSIN)</ccts:Name>
            <ccts:Description>Indication that the GS1 Global Shipment Identification Number has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="38">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked price</ccts:Name>
            <ccts:Description>Indication that the price has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="39">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with Serial Shipping Container Code (SSCC)</ccts:Name>
            <ccts:Description>Indication that the Serial Shipping Container Code has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="40">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with first freezing date</ccts:Name>
            <ccts:Description>Indication that the first freezing date has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="41">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with batch number</ccts:Name>
            <ccts:Description>Indication that the batch number has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="42">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with production date</ccts:Name>
            <ccts:Description>Indication that the production date has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="43">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with expiry date</ccts:Name>
            <ccts:Description>Indication that the expiry date has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="44">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with best before date</ccts:Name>
            <ccts:Description>Indication that the best before date has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="45">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with net weight</ccts:Name>
            <ccts:Description>Indication that the net weight has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="46">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with packaging date</ccts:Name>
            <ccts:Description>Indication that the packaging date has been marked on a package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="47">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with GS1 Global Returnable Asset Identifier (GRAI)</ccts:Name>
            <ccts:Description>Indication that the GS1 Global Returnable Asset Identifier (GRAI) has been marked on the package.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="48">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Marked with a greeting message</ccts:Name>
            <ccts:Description>Indication that the package has been marked with a greeting message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="49">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Price per unit of measure marked on product</ccts:Name>
            <ccts:Description>The price per unit of measure is marked on trade item.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ZZZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mutually defined</ccts:Name>
            <ccts:Description>A code assigned within a code list to be used on an interim basis and as defined among trading partners until a precise code can be assigned to the code list.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
