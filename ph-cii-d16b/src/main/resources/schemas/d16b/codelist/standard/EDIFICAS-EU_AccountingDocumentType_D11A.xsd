          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ledger</ccts:Name>
            <ccts:Description>The code is ledger.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reporting</ccts:Name>
            <ccts:Description>The code is reporting.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Trial balance</ccts:Name>
            <ccts:Description>The code is trial balance.</ccts:Description>
          </xsd:documentation>
