  <xsd:simpleType name="AccountingDocumentTypeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="BC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Bundle collection</ccts:Name>
            <ccts:Description>The code is bundle collection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="COA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Accounting account classification</ccts:Name>
            <ccts:Description>The code is accounting account classification.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Day book</ccts:Name>
            <ccts:Description>The code is day book.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="JL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Journal List</ccts:Name>
            <ccts:Description>The code is journal list.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="JN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Journal</ccts:Name>
