<!-- ======================================================================= -->
  <xsd:element name="TransportEquipmentCategoryCode" type="clm68053:TransportEquipmentCategoryCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: TransportEquipmentCategoryCodeContentType ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="TransportEquipmentCategoryCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="AA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ground equipment</ccts:Name>
            <ccts:Description>Ground equipment being fuelled or serviced.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Chain</ccts:Name>
            <ccts:Description>Chain used in the securing of cargo.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Temperature recorder</ccts:Name>
            <ccts:Description>Temperature recorder to provide a record of the actual temperature.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Body trailer</ccts:Name>
            <ccts:Description>The part of the vehicle where the cargo is loaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Slipsheet</ccts:Name>
            <ccts:Description>A cardboard platform used for holding product for storage or transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>No special equipment needed</ccts:Name>
            <ccts:Description>A code to indicate that there is no special equipment needed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Vessel hold</ccts:Name>
            <ccts:Description>A compartment forming part of a transport vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Flat rack</ccts:Name>
            <ccts:Description>Type of open container used for carrying objects.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Aircraft</ccts:Name>
            <ccts:Description>To indicate that the equipment is an aircraft.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Medical device</ccts:Name>
            <ccts:Description>A device used for medical purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Refrigerated container</ccts:Name>
            <ccts:Description>A refrigerated (reefer) container that is actively cooling the product.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Synthetic pallet 80*120cm</ccts:Name>
            <ccts:Description>A pallet with standard dimensions 80*120 centimetres made of synthetic material.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Synthetic pallet 100*120cm</ccts:Name>
            <ccts:Description>A standard pallet with standard dimensions 100*120 centimetres made of synthetic material.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Clothing hanger rack</ccts:Name>
            <ccts:Description>Equipment used to store and transport clothing in a hanging position.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Road/rail trailer</ccts:Name>
            <ccts:Description>Trailer designated for combined road/rail use.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Overhang wagon</ccts:Name>
            <ccts:Description>Empty rail wagon added to the loaded wagons, when goods are longer than the loaded wagon.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Un-containerized cargo (breakbulk)</ccts:Name>
            <ccts:Description>Cargoes in bulk not stuffed in equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Blocks</ccts:Name>
            <ccts:Description>A piece of equipment that is normally a piece of wood to fix cargo (e.g. coils) during transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Box pallet non-exchangeable</ccts:Name>
            <ccts:Description>A box pallet which cannot be exchanged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Truck being transported</ccts:Name>
            <ccts:Description>A road vehicle capable of carrying goods which is being carried on another means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Truck and trailer combination being transported</ccts:Name>
            <ccts:Description>A road vehicle capable of carrying goods with an attached trailer which is being carried on another means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Tractor and trailer being transported</ccts:Name>
            <ccts:Description>A trailer accompanied by a self-propelling tractor unit which is being carried on another means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Postal bag</ccts:Name>
            <ccts:Description>Bag intended primarily for the conveyance of postal items.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Letter Tray</ccts:Name>
            <ccts:Description>Tray intended primarily for the conveyance of letter mail items of size slightly exceeding C5, in which the items are stood on their long edge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Roller Cage</ccts:Name>
            <ccts:Description>Wheeled wire cage.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Flats Tray</ccts:Name>
            <ccts:Description>Tray intended primarily for the conveyance of letter mail items of from C5 up to in excess of C4, in which the items are stacked one on top of another. May also be used for letter mail smaller items stood on their long edge.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Out of bag parcel</ccts:Name>
            <ccts:Description>Postal item which is conveyed individually, with only the wrapping provided by the mailer for protection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Wheeled Platform</ccts:Name>
            <ccts:Description>Device, consisting of a flat surface mounted on a wheels, which is designed for the conveyance of items which may safely be stacked. Normally also equipped with a handle to allow the device to be easily manoeuvred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Container non-compliant with the Customs Convention on Containers</ccts:Name>
            <ccts:Description>The container is not compliant with the Customs Convention on Containers.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPY">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Box pallet EUR Y non exchangeable</ccts:Name>
            <ccts:Description>A euro-pallet of type Y that may not be exchanged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BPZ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Roll Pallet 1</ccts:Name>
            <ccts:Description>80 X 60 cm pallet on casters.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Barge</ccts:Name>
            <ccts:Description>Flat bottomed inland cargo vessel for canals and rivers with or without own propulsion for the purpose of transported goods. (Synonym: Lighter).</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BX">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Boxcar</ccts:Name>
            <ccts:Description>An enclosed railway goods wagon.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Chassis</ccts:Name>
            <ccts:Description>A wheeled carriage onto which an ocean container is mounted for inland conveyance.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Container</ccts:Name>
            <ccts:Description>Equipment item as defined by ISO for transport. It must be of: A) permanent character, strong enough for repeated use; B) designed to facilitate the carriage of goods, by one or more modes of transport, without intermediate reloading; C) fitted with devices for its ready handling, particularly.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Deadlight (panel)</ccts:Name>
            <ccts:Description>A set of panels fixed inside a porthole.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Roll Pallet 2</ccts:Name>
            <ccts:Description>120 X 60 cm pallet on casters.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Container gantry crane</ccts:Name>
            <ccts:Description>A fixed gantry crane ashore for container handling operations.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Mobile crane</ccts:Name>
            <ccts:Description>A mobile crane for handling operations.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Floating crane</ccts:Name>
            <ccts:Description>A crane mounted on a dedicated vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ship's equipment crane</ccts:Name>
            <ccts:Description>A crane mounted on a ship for handling operations.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Conveyor belt</ccts:Name>
            <ccts:Description>Rolling carpet belt.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Forklift</ccts:Name>
            <ccts:Description>Equipment device for handling and moving goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stacking equipment</ccts:Name>
            <ccts:Description>Equipment device for stacking goods.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Taillift</ccts:Name>
            <ccts:Description>Equipment device at the rear of a truck for loading and unloading cargo.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DPL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>On-board equipment</ccts:Name>
            <ccts:Description>Equipment permanently on board a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EFP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Exchangeable EUR flat pallet</ccts:Name>
            <ccts:Description>A flat euro-pallet that may be exchanged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="EYP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Exchangeable EUR Y box pallet</ccts:Name>
            <ccts:Description>A euro-pallet of type Y that may be exchanged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FPN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Flat pallet EUR non exchangeable</ccts:Name>
            <ccts:Description>A flat euro-pallet that may not be exchanged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="FPR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Flat pallet (railway property) non-exchangeable</ccts:Name>
            <ccts:Description>A non-exchangeable flat pallet owned by a railroad.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="IL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Lidded stackable rigid tray (CEN TS 14482:2002)</ccts:Name>
            <ccts:Description>Lidded stackable rigid tray compliant with CEN TS 14482:2002.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LAR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Lashing rope</ccts:Name>
            <ccts:Description>A rope for lashing cargo.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Load/unload device on equipment</ccts:Name>
            <ccts:Description>A mechanical device used in the loading and/or unloading of cargo into and from transport equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MPA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Movable panel</ccts:Name>
            <ccts:Description>A panel which can be moved.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pallet</ccts:Name>
            <ccts:Description>A platform on which goods can be stacked in order to facilitate the movement by a forklift or sling.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PBP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Identified private box pallet</ccts:Name>
            <ccts:Description>A box pallet identified as being privately owned.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PFP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Identified private flat pallet</ccts:Name>
            <ccts:Description>A flat pallet identified as being privately owned.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Platform</ccts:Name>
            <ccts:Description>A piece of equipment normally having a flat surface, or prepared for carrying cargo with a specific shape.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PPA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Protecting panel</ccts:Name>
            <ccts:Description>A panel used for protection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PST">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Portable stove</ccts:Name>
            <ccts:Description>A portable heating unit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Flat car</ccts:Name>
            <ccts:Description>A railway wagon without raised sides or ends.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reefer generator</ccts:Name>
            <ccts:Description>A generator used to control the temperature in temperature-controlled transport equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RGF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ground facility</ccts:Name>
            <ccts:Description>Storage tank or facility capable of receiving shipment of goods or commodity.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Rope</ccts:Name>
            <ccts:Description>Rope used in the securing of cargo.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Rail car</ccts:Name>
            <ccts:Description>To identify that the equipment is a rail car.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SPP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Identified special pallet</ccts:Name>
            <ccts:Description>A pallet identified as special.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="STR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Strap</ccts:Name>
            <ccts:Description>A narrow strip of flexible material.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="SW">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Swap body</ccts:Name>
            <ccts:Description>Rectangular equipment unit without wheels, which can be mounted on a chassis or positioned on legs.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TE">
