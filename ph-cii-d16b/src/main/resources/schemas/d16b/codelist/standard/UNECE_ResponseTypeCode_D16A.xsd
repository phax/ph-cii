<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm64343ResponseTypeCode="urn:un:unece:uncefact:codelist:standard:UNECE:ResponseTypeCode:D16A" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:ResponseTypeCode:D16A" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.2">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="ResponseTypeCode" type="clm64343ResponseTypeCode:ResponseTypeCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: ResponseTypeCodeContentType           ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="ResponseTypeCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="AA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Debit advice</ccts:Name>
            <ccts:Description>Receiver of the payment message needs to return a debit advice in response to the payment message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AB">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Message acknowledgement</ccts:Name>
            <ccts:Description>Indicates that an acknowledgement relating to receipt of message is required.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AC">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acknowledge - with detail and change</ccts:Name>
            <ccts:Description>Acknowledge complete including changes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AD">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acknowledge - with detail, no change</ccts:Name>
            <ccts:Description>Acknowledge complete without changes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Debit advice for each transaction</ccts:Name>
            <ccts:Description>A debit advice is requested for each transaction in the message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AF">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Debit advice/message acknowledgement</ccts:Name>
            <ccts:Description>The sender wishes to receive both a Debit Advice and an acknowledgement of receipt for a payment message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AG">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Authentication</ccts:Name>
            <ccts:Description>Authentication, by a party, of a document established for him by another party.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AH">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Debit advice/message acknowledgement for each transaction</ccts:Name>
            <ccts:Description>A debit advice and message acknowledgement are requested for each transaction in the message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AI">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acknowledge only changes</ccts:Name>
            <ccts:Description>Acknowledgement of changes only is required.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AJ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pending</ccts:Name>
            <ccts:Description>Indication that the referenced offer or transaction (e.g. cargo booking or quotation request) is being dealt with.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AK">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Return only TIR transport information</ccts:Name>
            <ccts:Description>The sender of the message expects only TIR (Transports Internationaux Routiers) transport information in return.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AL">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Return only declaration information</ccts:Name>
            <ccts:Description>Desc
The sender of the message expects only declaration information in return.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AM">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Return only guarantee information</ccts:Name>
            <ccts:Description>The sender of the message expects only guarantee information in return.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Return all information regarding guarantee</ccts:Name>
            <ccts:Description>The sender of the message expects all information regarding the guarantee in return.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AP">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Accepted</ccts:Name>
            <ccts:Description>Indication that the referenced offer or transaction (e.g., cargo booking or quotation request) has been accepted.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AQ">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Response expected</ccts:Name>
            <ccts:Description>The sender of the message expects a response.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Direct documentary credit collection</ccts:Name>
            <ccts:Description>Documentary credit collection forwarded directly.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AS">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Credit advice and message acknowledgement</ccts:Name>
            <ccts:Description>The receiver of the message is to acknowledge receipt of the message and sent a credit advice for each credit.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Conditionally accepted</ccts:Name>
            <ccts:Description>Indication that the referenced offer or transaction (e.g., cargo booking or quotation request) has been accepted under conditions indicated in this message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CO">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Confirmation of measurements</ccts:Name>
            <ccts:Description>Indication that the message contains the physical measurements on which the charges will be based.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NA">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>No acknowledgement needed</ccts:Name>
            <ccts:Description>Specifies that no acknowledgement is needed in response to this message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RE">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Rejected</ccts:Name>
            <ccts:Description>Indication that the referenced offer or transaction (e.g., cargo booking or quotation request) is not accepted.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UR">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Credit advice</ccts:Name>
            <ccts:Description>The message recipient is to send a credit advice in response to the message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="US">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acknowledgement when error</ccts:Name>
            <ccts:Description>An acknowledgement is requested when an error occurred.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UT">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Acknowledgment due to error</ccts:Name>
            <ccts:Description>An acknowledgment is sent because an error was identified in the received message.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UU">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Alternate date</ccts:Name>
            <ccts:Description>The solution proposed in the response applies to another date than the one requested.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="UV">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Alternate service</ccts:Name>
            <ccts:Description>The solution proposed in the response applies to another service than the one requested.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
